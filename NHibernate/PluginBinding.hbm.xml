<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Hamster.Plugin"
                   namespace="Hamster.Plugin.Configuration">

  <class name="PluginBinding" table="pluginBinding" lazy="false">
    <composite-id>
      <key-property name="TargetPlugin" column="targetPlugin"/>
      <key-property name="NativeName" column="nativeName"/>
    </composite-id>
    <property name="BoundPlugin" column="boundPlugin"/>
  </class>
  
</hibernate-mapping>